<?xml version="1.0"?>
<launch>
    <!-- Argument for camera -->
    <arg name="device_id"           default="0"/>
    <arg name="camera_info_file"    default="usbc_web_camera.yaml"/>
    <!-- Nodes -->
    <node name="camera" pkg="cv_camera" type="cv_camera_node" output="screen">
        <param name="camera_info_url" value="file://$(find svea_sensors)/params/robot_localization/$(arg camera_info_file)" />
        <param name="device_id"         value="$(arg device_id)"/>
    </node>
</launch>